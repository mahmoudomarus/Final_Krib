{
  "name": "uae-rental-backend",
  "version": "1.0.0",
  "description": "Backend API for UAE Rental Platform",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "postinstall": "npm run build && npx prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:seed": "npx tsx src/scripts/seedUsers.ts && npx tsx src/scripts/create60Properties.ts",
    "db:reset": "prisma migrate reset",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "heroku-postbuild": "npm run build && npx prisma generate",
    "deploy:prepare": "npm run build && npx prisma generate"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "@supabase/supabase-js": "^2.50.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "aws-sdk": "^2.1524.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.0.1",
    "google-auth-library": "^9.15.1",
    "helmet": "^7.2.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "^5.7.1",
    "recharts": "^2.8.0",
    "socket.io": "^4.8.1",
    "stripe": "^14.25.0",
    "tsx": "^4.19.1",
    "twilio": "^4.23.0",
    "winston": "^3.17.0",
    "zod": "^3.25.53"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.30",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "prisma",
    "postgresql",
    "rental",
    "uae"
  ],
  "author": "UAE Rental Platform Team",
  "license": "ISC"
}
